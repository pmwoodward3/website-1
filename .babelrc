{
  "presets": [
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "transform-class-properties",
    "ramda",
    "lodash"
  ],
  "env": {
    "start": {
      "presets": [
        "react",
        "stage-0",
        "es2015",
        "react-hmre"
      ]
    },
    "build": {
      "presets": [
        ["es2015", { "loose": true, "modules": false }],
        "stage-0",
        "react",
        "react-optimize"
      ],
    }
  }
}
